public class demo2 {
    public static void main(String[] args){
        int[] arr={1,7,2,9,5};
        int a=1;
        for(int i=0;i<5;i++)
        {
            if(arr[i]%2==0)
            {
                a=0;
                break;
            }
        }
        if(a==0)
        {
            System.out.println("it contains even numbers");
        }
        else
        {
            System.out.println("it contains odd numbers only");
        }
    }
}
